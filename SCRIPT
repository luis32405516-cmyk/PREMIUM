local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/luis32405516-cmyk/PREMIUM/refs/heads/main/LIBRARY"))()
local Window = Library:CreateWindow()

local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local RS = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local Lighting = game:GetService("Lighting")
local TeleportService = game:GetService("TeleportService")
local CG = game:GetService("CoreGui")

local FLYING = false
local iyflyspeed = 1
local InfJump = false
local noclipActive = false
local FLING_ENABLED = false
local ESP_Color = 0
local XRAY_ENABLED = false
local WalkSpeedValue = 16
local targetLobby = ""
local autoClicker = false
local autoWin = false
local autoTickets = false
local RainbowPlatform = false
local savedPos = nil
local networkPaused = nil
local eggNumber = "1"
local autoOpenEggs = false

local function mobilefly()
    FLYING = true
    local char = LP.Character
    local root = char:WaitForChild("HumanoidRootPart")
    local camera = workspace.CurrentCamera
    local control = require(LP.PlayerScripts:WaitForChild("PlayerModule"):WaitForChild("ControlModule"))
    local bv = Instance.new("BodyVelocity", root)
    bv.Name = "IY_Velocity"
    bv.MaxForce = Vector3.new(9e9,9e9,9e9)
    bv.Velocity = Vector3.new(0,0,0)
    local bg = Instance.new("BodyGyro", root)
    bg.Name = "IY_Gyro"
    bg.MaxTorque = Vector3.new(9e9,9e9,9e9)
    bg.P = 15000
    bg.D = 100
    task.spawn(function()
        while FLYING do
            RS.RenderStepped:Wait()
            if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid").PlatformStand = true end
            bg.CFrame = camera.CFrame
            local dir = control:GetMoveVector()
            local vel = Vector3.new(0,0,0)
            if dir.X ~= 0 then vel = vel + camera.CFrame.RightVector * (dir.X * (iyflyspeed * 50)) end
            if dir.Z ~= 0 then vel = vel - camera.CFrame.LookVector * (dir.Z * (iyflyspeed * 50)) end
            bv.Velocity = vel
        end
        bv:Destroy()
        bg:Destroy()
        if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid").PlatformStand = false end
    end)
end

local Mov = Window:CreateTab("âš¡ MOVIMIENTO")
Mov:CreateSlider("SPEED âš¡", 16, 500, function(v) WalkSpeedValue = v; if LP.Character then LP.Character.Humanoid.WalkSpeed = v end end)
Mov:CreateSwitch("Fly Mobile (IY)", function(s) if s then mobilefly() else FLYING = false end end)
Mov:CreateSlider("Fly Speed", 1, 20, function(v) iyflyspeed = v end)
Mov:CreateSwitch("Noclip ğŸ‘»", function(s) noclipActive = s; if s then _G.Nc = RS.Stepped:Connect(function() if LP.Character then for _,v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end end end) else if _G.Nc then _G.Nc:Disconnect() end end end)
Mov:CreateSwitch("Infinite Jump ğŸ¦˜", function(s) InfJump = s end)
Mov:CreateSwitch("Walk Fling ğŸŒ€", function(s) FLING_ENABLED = s; task.spawn(function() while FLING_ENABLED do RS.Heartbeat:Wait(); if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then local r = LP.Character.HumanoidRootPart; local v = r.Velocity; r.Velocity = v * 10000 + Vector3.new(0,10000,0); RS.RenderStepped:Wait(); r.Velocity = v end end end) end)

Mov:CreateButton("GUARDAR / TELEPORT ğŸ“", function()
    local char = LP.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        if not savedPos then
            savedPos = char.HumanoidRootPart.CFrame
            Library:Notify("SISTEMA", "UbicaciÃ³n Guardada âœ…")
        else
            char.HumanoidRootPart.CFrame = savedPos
            savedPos = nil
            Library:Notify("SISTEMA", "Teletransportado con Ã©xito ğŸš€")
        end
    end
end)

Mov:CreateSwitch("Plataforma Rainbow ğŸŒˆ", function(s)
    RainbowPlatform = s
    if s then
        local plat = Instance.new("Part", workspace)
        plat.Size = Vector3.new(5, 1, 5)
        plat.Anchored = true
        plat.CanCollide = true
        plat.Name = "LuisPlatform"
        task.spawn(function()
            while RainbowPlatform do
                RS.RenderStepped:Wait()
                if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
                    plat.CFrame = LP.Character.HumanoidRootPart.CFrame * CFrame.new(0, -3.5, 0)
                    plat.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                end
            end
            plat:Destroy()
        end)
    end
end)

if game.PlaceId == 14236123211 then
    local Spec = Window:CreateTab("ğŸ® EVENTO")
    Spec:CreateSwitch("Auto Golpes ğŸ¥Š", function(s)
        autoClicker = s
        task.spawn(function()
            while autoClicker do
                pcall(function() game:GetService("ReplicatedStorage").Events.DamageIncreaseOnClickEvent:FireServer() end)
                task.wait(0.01)
            end
        end)
    end)
    Spec:CreateSwitch("Auto Win (Push) ğŸ†", function(s)
        autoWin = s
        task.spawn(function()
            while autoWin do
                pcall(function() game:GetService("ReplicatedStorage").Events.PushEvent:FireServer(true) end)
                task.wait(0.1)
            end
        end)
    end)
end

if game.PlaceId == 11708967881 then
    local EggTab = Window:CreateTab("ğŸ¥š HUEVOS")
    EggTab:CreateTextBox("NÃšMERO DE HUEVO", function(v) eggNumber = v end)
    EggTab:CreateSwitch("Auto Abrir Huevos", function(s)
        autoOpenEggs = s
        task.spawn(function()
            while autoOpenEggs do
                local args = {
                    [1] = tostring(eggNumber),
                    [2] = true
                }
                pcall(function()
                    game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Egg"):WaitForChild("Server"):WaitForChild("Purchase"):InvokeServer(unpack(args))
                end)
                task.wait(1)
            end
        end)
    end)

    local ThrowTab = Window:CreateTab("ğŸš€ LANZAMIENTO")
    ThrowTab:CreateButton("LANZAR", function()
        pcall(function()
            game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Throw"):WaitForChild("Server"):WaitForChild("Request"):FireServer()
        end)
    end)
    
    ThrowTab:CreateButton("DETENER", function()
        pcall(function()
            local remotePath = game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Throw"):WaitForChild("Server")
            remotePath:WaitForChild("Landed"):InvokeServer(true)
            remotePath:WaitForChild("Request"):FireServer()
        end)
    end)
end

if game.PlaceId == 83347992878150 or tostring(game.PlaceId) == "83347992878150" then
    local TicketTab = Window:CreateTab("ğŸŸï¸ TICKETS")
    TicketTab:CreateSwitch("Auto Farm Tickets", function(s)
        autoTickets = s
        task.spawn(function()
            while autoTickets do
                local folder = workspace:FindFirstChild("SpinTicketFolder")
                if folder then
                    local tickets = {}
                    for _, v in pairs(folder:GetChildren()) do
                        if v.Name == "Ticket" then table.insert(tickets, v) end
                    end
                    if #tickets > 0 and LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
                        local target = tickets[math.random(1, #tickets)]
                        if target:IsA("Model") then LP.Character.HumanoidRootPart.CFrame = target:GetModelCFrame()
                        elseif target:IsA("BasePart") then LP.Character.HumanoidRootPart.CFrame = target.CFrame end
                    end
                end
                task.wait(2)
            end
        end)
    end)
    TicketTab:CreateButton("VOLVER AL LOBBY ğŸ ", function() TeleportService:Teleport(13457104744, LP) end)
end

local Vis = Window:CreateTab("ğŸ‘ï¸ VISUALES")
Vis:CreateSwitch("ESP Maestro ğŸŒˆ", function(s) _G.ESP = s; task.spawn(function() while _G.ESP do local color = (ESP_Color == 0) and Color3.fromHSV((tick()*0.5)%1,1,1) or Color3.fromHSV(ESP_Color/100,1,1); for _, p in pairs(Players:GetPlayers()) do if p ~= LP and p.Character then local hl = p.Character:FindFirstChild("LuisESP") or Instance.new("Highlight", p.Character); hl.Name = "LuisESP"; hl.FillColor = color end end; task.wait() end; for _, p in pairs(Players:GetPlayers()) do if p.Character and p.Character:FindFirstChild("LuisESP") then p.Character.LuisESP:Destroy() end end end) end)
Vis:CreateSlider("Color ESP (0=Rainbow)", 0, 100, function(v) ESP_Color = v end)
Vis:CreateSwitch("X-Ray (Paredes) ğŸ’", function(s) XRAY_ENABLED = s; for _,v in pairs(workspace:GetDescendants()) do if v:IsA("BasePart") and not v.Parent:FindFirstChild("Humanoid") then v.Transparency = s and 0.5 or 0 end end end)
Vis:CreateSlider("Hora", 0, 24, function(v) if v ~= 0 then Lighting.ClockTime = v end end)

local Ser = Window:CreateTab("ğŸŒ SERVER")
Ser:CreateButton("REJOIN ğŸ”„", function() TeleportService:Teleport(game.PlaceId, LP) end)
Ser:CreateButton("SERVER HOP ğŸš€", function() 
    local srv = {}
    local res = game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Desc&limit=100")
    for _, s in pairs(game:GetService("HttpService"):JSONDecode(res).data) do if s.playing < s.maxPlayers and s.id ~= game.JobId then table.insert(srv, s.id) end end
    if #srv > 0 then TeleportService:TeleportToPlaceInstance(game.PlaceId, srv[math.random(1, #srv)], LP) end 
end)
Ser:CreateTextBox("ID DEL LOBBY", function(v) targetLobby = v end)
Ser:CreateButton("COPIAR ID JUEGO ğŸ“‹", function() setclipboard(tostring(game.PlaceId)) end)
Ser:CreateButton("IR AL LOBBY ğŸ ", function() if targetLobby ~= "" then TeleportService:Teleport(tonumber(targetLobby), LP) end end)

local Sys = Window:CreateTab("ğŸ› ï¸ SISTEMA")
Sys:CreateSwitch("FPS Booster ğŸš€", function(s) if s then for _,v in pairs(game:GetDescendants()) do if v:IsA("Part") or v:IsA("MeshPart") then v.Material = Enum.Material.SmoothPlastic; v.Reflectance = 0 end end; Lighting.GlobalShadows = false; settings().Rendering.QualityLevel = 1 end end)
Sys:CreateSwitch("Anti-AFK ğŸš«ğŸ’¤", function(s) if s then _G.AFK = LP.Idled:Connect(function() game:GetService("VirtualUser"):CaptureController(); game:GetService("VirtualUser"):ClickButton2(Vector2.new()) end) else if _G.AFK then _G.AFK:Disconnect() end end end)

Sys:CreateSwitch("Anti-Gameplay Paused ğŸ›‘", function(s)
    if s then
        pcall(function() if networkPaused then networkPaused:Disconnect() end end)
        networkPaused = CG.RobloxGui.ChildAdded:Connect(function(obj)
            if obj.Name == "CoreScripts/NetworkPause" then
                obj:Destroy()
            end
        end)
        if CG.RobloxGui:FindFirstChild("CoreScripts/NetworkPause") then
            CG.RobloxGui["CoreScripts/NetworkPause"]:Destroy()
        end
    else
        if networkPaused then networkPaused:Disconnect(); networkPaused = nil end
    end
end)

UIS.JumpRequest:Connect(function() if InfJump and LP.Character then LP.Character.Humanoid:ChangeState("Jumping") end end)
LP.CharacterAdded:Connect(function(c) c:WaitForChild("Humanoid").WalkSpeed = WalkSpeedValue end)
