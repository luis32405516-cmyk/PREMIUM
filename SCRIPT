local LOGIN_URL = "https://raw.githubusercontent.com/luis32405516-cmyk/PREMIUM/refs/heads/main/LOGIN"

local function verify(key)
    if not key or key == "" then return false end
    local url = "https://api.platoboost.net/public/whitelist/"..service.."?identifier="..LP.UserId.."&key="..key
    local success, response = pcall(function() 
        return fRequest({Url = url, Method = "GET"}) 
    end)
    if success and response.StatusCode == 200 then
        local decoded = HttpService:JSONDecode(response.Body)
        return decoded.success and decoded.data.valid
    end
    return false
end

local hasAccess = false
if LP.UserId == TU_ID or LP.UserId == AMIGO_ID then
    hasAccess = true
elseif isfile(FILE_PATH) then
    if verify(readfile(FILE_PATH)) then
        hasAccess = true
    end
end

if not hasAccess then
    loadstring(game:HttpGet(LOGIN_URL))()
    return
end

