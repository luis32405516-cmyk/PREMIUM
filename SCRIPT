local function findNode(pathStr)
    local parts = string.split(pathStr, ".")
    local current = workspace
    for _, name in pairs(parts) do
        current = current:FindFirstChild(name)
        if not current then return nil end
    end
    return current
end

local function TogglePlatform(state)
    RainbowPlatform = state
    if state then
        if not plat then
            plat = Instance.new("Part")
            plat.Size = Vector3.new(10, 1, 10)
            plat.Anchored = true
            plat.CanCollide = true
            plat.Transparency = 0.5
            plat.Name = "LuisPlatform"
            plat.Parent = workspace
            task.spawn(function()
                while RainbowPlatform do
                    RS.Stepped:Wait()
                    if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
                        local hrp = LP.Character.HumanoidRootPart
                        plat.CFrame = CFrame.new(hrp.Position.X, hrp.Position.Y - 4.5, hrp.Position.Z)
                        plat.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                    end
                end
                if plat then plat:Destroy(); plat = nil end
            end)
        end
    elseif plat then
        plat:Destroy()
        plat = nil
    end
end

local function tp(target)
    if not RainbowPlatform then TogglePlatform(true) end
    if target and LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
        local cf = target:IsA("BasePart") and target.CFrame or target:IsA("Model") and target.PrimaryPart and target.PrimaryPart.CFrame or target:IsA("Model") and target:GetModelCFrame()
        if cf then 
            LP.Character.HumanoidRootPart.CFrame = cf 
        end
    end
end

local function mobilefly()
    FLYING = true
    local char = LP.Character
    local root = char:WaitForChild("HumanoidRootPart")
    local camera = workspace.CurrentCamera
    local control = require(LP.PlayerScripts:WaitForChild("PlayerModule"):WaitForChild("ControlModule"))
    local bv = Instance.new("BodyVelocity", root)
    bv.Name = "IY_Velocity"
    bv.MaxForce = Vector3.new(9e9,9e9,9e9)
    bv.Velocity = Vector3.new(0,0,0)
    local bg = Instance.new("BodyGyro", root)
    bg.Name = "IY_Gyro"
    bg.MaxTorque = Vector3.new(9e9,9e9,9e9)
    bg.P = 15000
    bg.D = 100
    task.spawn(function()
        while FLYING do
            RS.RenderStepped:Wait()
            if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid").PlatformStand = true end
            bg.CFrame = camera.CFrame
            local dir = control:GetMoveVector()
            local vel = Vector3.new(0,0,0)
            if dir.X ~= 0 then vel = vel + camera.CFrame.RightVector * (dir.X * (iyflyspeed * 50)) end
            if dir.Z ~= 0 then vel = vel - camera.CFrame.LookVector * (dir.Z * (iyflyspeed * 50)) end
            bv.Velocity = vel
        end
        bv:Destroy()
        bg:Destroy()
        if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid").PlatformStand = false end
    end)
end

local Mov = Window:CreateTab("‚ö° MOVIMIENTO")
Mov:CreateSwitch("Activar Speed ‚ö°", function(s) 
    SpeedEnabled = s
    if LP.Character then 
        LP.Character.Humanoid.WalkSpeed = s and WalkSpeedValue or 16 
    end 
end)
Mov:CreateSlider("SPEED VALUE", 16, 500, function(v) 
    WalkSpeedValue = v
    if SpeedEnabled and LP.Character then 
        LP.Character.Humanoid.WalkSpeed = v 
    end 
end)
Mov:CreateSwitch("Fly Mobile (IY)", function(s) if s then mobilefly() else FLYING = false end end)
Mov:CreateSlider("Fly Speed", 1, 20, function(v) iyflyspeed = v end)
Mov:CreateSwitch("Noclip üëª", function(s) noclipActive = s; if s then _G.Nc = RS.Stepped:Connect(function() if LP.Character then for _,v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end end end) else if _G.Nc then _G.Nc:Disconnect() end end end)
Mov:CreateSwitch("Infinite Jump ü¶ò", function(s) InfJump = s end)
Mov:CreateSwitch("Walk Fling üåÄ", function(s) FLING_ENABLED = s; task.spawn(function() while FLING_ENABLED do RS.Heartbeat:Wait(); if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then local r = LP.Character.HumanoidRootPart; local v = r.Velocity; r.Velocity = v * 10000 + Vector3.new(0,10000,0); RS.RenderStepped:Wait(); r.Velocity = v end end end) end)

Mov:CreateButton("GUARDAR / TELEPORT üìç", function()
    local char = LP.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        if not savedPos then
            savedPos = char.HumanoidRootPart.CFrame
            Library:Notify("SISTEMA", "Ubicaci√≥n Guardada ‚úÖ")
        else
            tp(savedPos)
            savedPos = nil
            Library:Notify("SISTEMA", "Teletransportado con √©xito üöÄ")
        end
    end
end)

