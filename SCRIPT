                if obj then tp(obj) end
                task.wait(0.1)
            end
        end)
    end)
    MainTab:CreateSwitch("NPC üîé OBJETOS", function(s)
        multiTeleporting = s
        task.spawn(function()
            local names = {"RevealPickupsSteveEagle", "RevealPickupsSteveGecko", "RevealPickupsSteveMantisShrimp", "RevealPickupsSteveSnowLeopard"}
            while multiTeleporting do
                for _, name in pairs(names) do
                    if not multiTeleporting then break end
                    local slot = workspace:FindFirstChild("PickupSlots") and workspace.PickupSlots:FindFirstChild(name)
                    if slot and slot:FindFirstChild("Default") then tp(slot.Default) end
                    task.wait(2.0)
                end
            end
        end)
    end)
    MainTab:CreateButton("GANAR üèÅ", function()
        local dest = workspace:FindFirstChild("Utility") and workspace.Utility:FindFirstChild("Teleporters") and workspace.Utility.Teleporters:FindFirstChild("Portal_to_Wc")
        if dest then tp(dest.Portal["Canvas Clouds"]:FindFirstChild("Cylinder")) end
    end)
    local StickTab = Window:CreateTab("üèÅ CHECKPOINTS")
    StickTab:CreateButton("‚è™ ANTERIOR", function()
        stickerIndex = math.max(1, stickerIndex - 1)
        local node = findNode(stickerPaths1[stickerIndex])
        if node then tp(node:FindFirstChild("Touch")) end
    end)
    StickTab:CreateButton("‚èπÔ∏è RESET (1)", function()
        stickerIndex = 1
        local node = findNode(stickerPaths1[stickerIndex])
        if node then tp(node:FindFirstChild("Touch")) end
    end)
    StickTab:CreateButton("‚è© SIGUIENTE", function()
        stickerIndex = math.min(#stickerPaths1, stickerIndex + 1)
        local node = findNode(stickerPaths1[stickerIndex])
        if node then tp(node:FindFirstChild("Touch")) end
    end)
    StickTab:CreateSwitch("AUTO AVANZAR ‚è©", function(s)
        autoSticker = s
        task.spawn(function()
            while autoSticker do
                stickerIndex = stickerIndex >= #stickerPaths1 and 1 or stickerIndex + 1
                local node = findNode(stickerPaths1[stickerIndex])
                if node then tp(node:FindFirstChild("Touch")) end
                task.wait(2.0)
            end
        end)
    end)
end

