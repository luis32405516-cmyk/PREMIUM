            end
        end)
    end)
    Spec:CreateSwitch("Auto Win (Push) üèÜ", function(s)
        autoWin = s
        task.spawn(function()
            while autoWin do
                pcall(function() game:GetService("ReplicatedStorage").Events.PushEvent:FireServer(true) end)
                task.wait(0.1)
            end
        end)
    end)
end

if game.PlaceId == 11708967881 then
    local EggTab = Window:CreateTab("ü•ö HUEVOS")
    EggTab:CreateTextBox("N√öMERO DE HUEVO", function(v) eggNumber = v end)
    EggTab:CreateSwitch("Auto Abrir Huevos", function(s)
        autoOpenEggs = s
        task.spawn(function()
            while autoOpenEggs do
                local args = {[1] = tostring(eggNumber), [2] = true}
                pcall(function() game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Egg"):WaitForChild("Server"):WaitForChild("Purchase"):InvokeServer(unpack(args)) end)
                task.wait(1)
            end
        end)
    end)
    
    EggTab:CreateSwitch("BORRAR 1x1 (0.5s) üóëÔ∏è", function(s)
        massDeletePets = s
        task.spawn(function()
            while massDeletePets do
                pcall(function()
                    local petList = LP.PlayerGui.Main.Center.Pets.PetInventory.PetList
                    for _, item in pairs(petList:GetChildren()) do
                        if not massDeletePets then break end
                        if item:IsA("GuiObject") and item.Name ~= "UIListLayout" and item.Name ~= "Template" then
                            local args = { { item.Name } }
                            game:GetService("ReplicatedStorage").Remote.Pet.Server.Delete:FireServer(unpack(args))
                            task.wait(0.5)
                        end
                    end
                end)
                task.wait(0.1)
            end
        end)
    end)

    local ThrowTab = Window:CreateTab("üöÄ LANZAMIENTO")
    ThrowTab:CreateButton("LANZAR", function() pcall(function() game:GetService("ReplicatedStorage").Remote.Throw.Server.Request:FireServer() end) end)
    ThrowTab:CreateButton("DETENER", function() pcall(function() local r = game:GetService("ReplicatedStorage").Remote.Throw.Server; r.Landed:InvokeServer(true); r.Request:FireServer() end) end)

    local FarmTab = Window:CreateTab("üåÄ AUTO FARM")
    FarmTab:CreateSwitch("AUTO REBIRTH", function(s) autoRebirth = s; task.spawn(function() while autoRebirth do pcall(function() game:GetService("ReplicatedStorage").Remote.Player.Server.AttemptRebirth:InvokeServer() end) task.wait(1) end end) end)
    FarmTab:CreateSwitch("AUTO SUPER REBIRTH", function(s) autoSuperRebirth = s; task.spawn(function() while autoSuperRebirth do pcall(function() game:GetService("ReplicatedStorage").Remote.Player.Server.AttemptSuperRebirth:InvokeServer() end) task.wait(5) end end) end)
    FarmTab:CreateButton("RESET PTOS SUPER REBIRTH", function() pcall(function() game:GetService("ReplicatedStorage").Remote.Player.Server.AttemptResetSuperRebirth:InvokeServer() end) end)
    FarmTab:CreateSwitch("CLAIN TIME DAY REWARD", function(s) autoClaimRewards = s; task.spawn(function() while autoClaimRewards do pcall(function() game:GetService("ReplicatedStorage").Remote.Player.Server.ClaimTimedReward:InvokeServer() end) task.wait(10) end end) end)
    FarmTab:CreateSwitch("AUTO COLLECT STARS ‚≠ê", function(s) autoStars = s; task.spawn(function() while autoStars do if workspace:FindFirstChild("Stars") then for _, star in pairs(workspace.Stars:GetChildren()) do if not autoStars then break end game:GetService("ReplicatedStorage").Remote.Star.Server.Collect:FireServer(star.Name) end end task.wait(1) end end) end)
end

