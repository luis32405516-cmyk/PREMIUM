local function findNode(path)
    local node = workspace
    for seg in string.gmatch(path, "[^%.]+") do
        node = node and node:FindFirstChild(seg)
    end
    return node
end

local function tp(target)
    if target and LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
        local cf = target:IsA("BasePart") and target.CFrame or target:IsA("Model") and target.PrimaryPart and target.PrimaryPart.CFrame
        if cf then 
            LP.Character.HumanoidRootPart.CFrame = cf 
            if plat then plat.CFrame = cf * CFrame.new(0, -platformYOffset, 0) end
        end
    end
end

local function mobilefly()
    FLYING = true
    local char = LP.Character
    local root = char:WaitForChild("HumanoidRootPart")
    local camera = workspace.CurrentCamera
    local control = require(LP.PlayerScripts:WaitForChild("PlayerModule"):WaitForChild("ControlModule"))
    local bv = Instance.new("BodyVelocity", root)
    bv.Name = "IY_Velocity"
    bv.MaxForce = Vector3.new(9e9,9e9,9e9)
    bv.Velocity = Vector3.new(0,0,0)
    local bg = Instance.new("BodyGyro", root)
    bg.Name = "IY_Gyro"
    bg.MaxTorque = Vector3.new(9e9,9e9,9e9)
    bg.P = 15000
    bg.D = 100
    task.spawn(function()
        while FLYING do
            RS.RenderStepped:Wait()
            if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid").PlatformStand = true end
            bg.CFrame = camera.CFrame
            local dir = control:GetMoveVector()
            local vel = Vector3.new(0,0,0)
            if dir.X ~= 0 then vel = vel + camera.CFrame.RightVector * (dir.X * (iyflyspeed * 50)) end
            if dir.Z ~= 0 then vel = vel - camera.CFrame.LookVector * (dir.Z * (iyflyspeed * 50)) end
            bv.Velocity = vel
        end
        bv:Destroy()
        bg:Destroy()
        if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid").PlatformStand = false end
    end)
end

local Mov = Window:CreateTab("‚ö° MOVIMIENTO")
Mov:CreateSlider("SPEED ‚ö°", 16, 500, function(v) WalkSpeedValue = v; if LP.Character then LP.Character.Humanoid.WalkSpeed = v end end)
Mov:CreateSwitch("Fly Mobile (IY)", function(s) if s then mobilefly() else FLYING = false end end)
Mov:CreateSlider("Fly Speed", 1, 20, function(v) iyflyspeed = v end)
Mov:CreateSwitch("Noclip üëª", function(s) noclipActive = s; if s then _G.Nc = RS.Stepped:Connect(function() if LP.Character then for _,v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end end end) else if _G.Nc then _G.Nc:Disconnect() end end end)
Mov:CreateSwitch("Infinite Jump ü¶ò", function(s) InfJump = s end)
Mov:CreateSwitch("Walk Fling üåÄ", function(s) FLING_ENABLED = s; task.spawn(function() while FLING_ENABLED do RS.Heartbeat:Wait(); if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then local r = LP.Character.HumanoidRootPart; local v = r.Velocity; r.Velocity = v * 10000 + Vector3.new(0,10000,0); RS.RenderStepped:Wait(); r.Velocity = v end end end) end)

Mov:CreateButton("GUARDAR / TELEPORT üìç", function()
    local char = LP.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        if not savedPos then
            savedPos = char.HumanoidRootPart.CFrame
            Library:Notify("SISTEMA", "Ubicaci√≥n Guardada ‚úÖ")
        else
            char.HumanoidRootPart.CFrame = savedPos
            savedPos = nil
            Library:Notify("SISTEMA", "Teletransportado con √©xito üöÄ")
        end
    end
end)

Mov:CreateSwitch("Plataforma Inteligente üåà", function(s)
    RainbowPlatform = s
    if s then
        plat = Instance.new("Part", workspace)
        plat.Size = Vector3.new(6, 1, 6)
        plat.Anchored = true
        plat.CanCollide = true
        plat.Name = "LuisPlatform"
        task.spawn(function()
            while RainbowPlatform do
                RS.RenderStepped:Wait()
                if LP.Character and LP.Character:FindFirstChild("HumanoidRootPart") then
                    local hrp = LP.Character.HumanoidRootPart
                    plat.CFrame = CFrame.new(hrp.Position.X, hrp.Position.Y - platformYOffset, hrp.Position.Z)
                    plat.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                end
            end
            if plat then plat:Destroy(); plat = nil end
        end)
    end
end)

if game.PlaceId == 113766574853716 or tostring(game.PlaceId) == "113766574853716" then
    local MainTab = Window:CreateTab("üéØ PRINCIPAL")
    MainTab:CreateSwitch("üîé OBJETOS", function(s)
        teleporting = s
        task.spawn(function()
            while teleporting do
                local obj = workspace:FindFirstChild("Pickups") and workspace.Pickups:FindFirstChild("Default")
                if obj then tp(obj) end
                task.wait(TELEPORT_INTERVAL)
            end
        end)
    end)
    MainTab:CreateSlider("Velocidad Objetos", 0.1, 5, function(v) TELEPORT_INTERVAL = v end)

    MainTab:CreateSwitch("NPC üîé OBJETOS", function(s)
        multiTeleporting = s
        task.spawn(function()
            local names = {"RevealPickupsSteveEagle", "RevealPickupsSteveGecko", "RevealPickupsSteveMantisShrimp", "RevealPickupsSteveSnowLeopard"}
            while multiTeleporting do
                for _, name in pairs(names) do
                    if not multiTeleporting then break end
                    local slot = workspace:FindFirstChild("PickupSlots") and workspace.PickupSlots:FindFirstChild(name)
                    if slot and slot:FindFirstChild("Default") then tp(slot.Default) end
                    task.wait(MULTI_TELEPORT_INTERVAL)
                end
            end
        end)
    end)
    MainTab:CreateSlider("Velocidad NPC", 0.5, 10, function(v) MULTI_TELEPORT_INTERVAL = v end)

    MainTab:CreateButton("GANAR üèÅ", function()
        local dest = workspace:FindFirstChild("Utility") and workspace.Utility:FindFirstChild("Teleporters") and workspace.Utility.Teleporters:FindFirstChild("Portal_to_Wc")
        if dest then tp(dest.Portal["Canvas Clouds"]:FindFirstChild("Cylinder")) end
    end)

    local StickTab = Window:CreateTab("üìú STICKERS")
    StickTab:CreateButton("‚è™ ANTERIOR", function()
        stickerIndex = math.max(1, stickerIndex - 1)
        local node = findNode(stickerPaths[stickerIndex])
        if node then tp(node:FindFirstChild("Touch")) end
    end)
    StickTab:CreateButton("‚èπÔ∏è RESET (1)", function()
        stickerIndex = 1
        local node = findNode(stickerPaths[stickerIndex])
        if node then tp(node:FindFirstChild("Touch")) end
    end)
    StickTab:CreateButton("‚è© SIGUIENTE", function()
        stickerIndex = math.min(#stickerPaths, stickerIndex + 1)
        local node = findNode(stickerPaths[stickerIndex])
        if node then tp(node:FindFirstChild("Touch")) end
    end)
    StickTab:CreateSwitch("AUTO AVANZAR ‚è©", function(s)
        autoSticker = s
        task.spawn(function()
            while autoSticker do
                stickerIndex = stickerIndex >= #stickerPaths and 1 or stickerIndex + 1
                local node = findNode(stickerPaths[stickerIndex])
                if node then tp(node:FindFirstChild("Touch")) end
                task.wait(STICKER_INTERVAL)
            end
        end)
    end)
    StickTab:CreateSlider("Velocidad Auto", 0.5, 10, function(v) STICKER_INTERVAL = v end)
end

