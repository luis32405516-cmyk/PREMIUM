local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/luis32405516-cmyk/PREMIUM/refs/heads/main/Yy"))()
local Window = Library:CreateWindow()

local MainTab = Window:CreateTab("üéØ DISPARADOR")

local function TriggerUgcData()
    local success, result = pcall(function()
        -- Esta es la ruta exacta seg√∫n tu imagen y descripci√≥n
        local remote = game:GetService("ReplicatedStorage").Packages.Knit.Services.ROTARYUgcLeaderboardService.RF.GetUgcScreenData
        return remote:InvokeServer()
    end)

    if success then
        print("Datos recibidos correctamente")
        Library:Notify("SISTEMA", "Datos de UGC obtenidos (Ver Consola)")
        -- Aqu√≠ puedes manejar el 'result' si el servidor devuelve tablas de datos
    else
        warn("Error al disparar el Remote: ", result)
        Library:Notify("ERROR", "No se pudo obtener la informaci√≥n")
    end
end

MainTab:CreateButton("OBTENER DATOS UGC üõ∞Ô∏è", function()
    TriggerUgcData()
end)

MainTab:CreateSwitch("AUTO-REFRESCAR UGC üîÅ", function(s)
    _G.AutoUgc = s
    task.spawn(function()
        while _G.AutoUgc do
            TriggerUgcData()
            task.wait(10) -- Espera 10 segundos entre cada petici√≥n para evitar baneo
        end
    end)
end)
